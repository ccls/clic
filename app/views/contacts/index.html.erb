<h2>Study Contact Info</h2>

<% unless @studies.to_a.empty? %>
  <table id='contacts'>
<thead><tr><th>Name</th><th>Contact Info</th></tr></thead>
<tbody>
  <%= render :partial => 'contacts/contact', :collection => @studies %>
  </tbody></table><!-- id='contacts' -->
<% else %>
  <p>Sorry, no contacts yet.</p>
<% end %>
